int main() {
     int i,n;
    scanf("%d",&n);
    int arr[n];
    for(i=0;i<n;i++)
    {
    scanf("%d ",&arr[i]);
    }
    int lar=arr[0];
    for(i=0;i<n;i++)
    {
            if(arr[i]>lar)
        {
            lar=arr[i];
        }
    }   
    int min=arr[0];
    for(i=0;i<n;i++)
    {
            if(arr[i]<min)
        {
            min=arr[i];
        }
    }   
        printf("%d %d",min,lar);
    return 0;
}
