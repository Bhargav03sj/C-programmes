#include <stdio.h>
int main(){
    int x;
    printf("marks = ");
    scanf("%d",&x);
    if((x>=90) && (x<=100)){
        printf("your obtained A+");
    }
    else if((x>=80) && (x<=89)){
        printf("your obtained A");
    }
    else if((x>=70)&& (x<=79)){
        printf("your obtained B+");
    }
    else if((x>=60)&& (x<=69)){
        printf("your obtained B");
    }
    else if((x>=50)&& (x<=59)){
        printf("your obtained C");
    }
    else if((x<50)&& (x>=0)){
        printf("your obtained F");
    }
    else{
        printf(" error !!!!!!! maximum number or total marks are equal or less than 100");
    }
    
    return 0;
}
