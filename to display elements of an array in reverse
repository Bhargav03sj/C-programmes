#include<stdio.h>
int main(){
     int size,i;
     printf("Enter array size = ");
     scanf("%d",&size);
     int stu[size];
     printf("Enter students roll no. = ");

     for(i=0;i<size;i++)
     {
    scanf("%d",&stu[i]);
     }
     for(i=size-1;i>=0;i--)
     {
    printf("%d\t",stu[i]);
     }
     
    return 0;


    
}
