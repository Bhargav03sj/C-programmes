// programm to find larger number with if else statement
#include <stdio.h>
int main(){
    int x,y;
    printf("value of x = ");
    scanf("%d",&x);
    printf("value of y = ");
    scanf("%d",&y);
    if(x>y){
        printf("x is larger");
    }
    else{
        printf("y is largest");
    }
    return 0;

}
