#include <stdio.h>
int main(){
    char ch;
    printf("ENTER A VALUE : ");
    scanf("%c",&ch);
    if((ch>=65 && ch<90) || (ch>97 && ch<122)){
    printf("Its a alphabet");
}
    else if(ch>=48 && ch<=57){
        printf("its a digit");
    }
    else {
        printf("its a symbol");
    }
    return 0;
}
