#include<stdio.h>
#include<limits.h>
int main(){
    int n,i,num,max=INT_MIN;
    printf("total numbers = ");
    scanf("%d",&n);
    for(i=1;i<=n;i++){
        printf("enter numbers = ");
        scanf("%d",&num);
        if(num>max){
        
            max=num;}
    }
    printf("largest no. is %d",max);

    return 0;
}
