#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
    int main(){
     int i,n,count=0,count1=0;
    scanf("%d",&n);
    int arr[n];
    for(i=0;i<n;i++)
    {
    scanf("%d ",&arr[i]);
    }
        for(i=0;i<n;i++){
     if(arr[i]%2==0)
     {
         count++;
         }
        }
         printf("%d ",count);
        for(i=0;i<n;i++)
    {
    scanf("%d ",&arr[i]);
    }
        for(i=0;i<n;i++){
     if(arr[i]%2!=0)
     {
         count1++;
         }
        }
         printf("%d ",count1);
    return 0;
        }
